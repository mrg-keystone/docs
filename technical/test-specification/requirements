[DTO] RecordingSetResponseDto {}

[DTO] RecordingDto {}

[DTO] GenieCredentialsDto {
  genieAcctId: string, genieAcctPass: string,
  providerName: string, externalRecordingId: string,
}

[DTO] UrlDto {
  value: string
}
