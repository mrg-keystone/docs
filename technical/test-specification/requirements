[REQ] recording.set(GetRecordingDto): RecordingSetResponseDto
    id.create(providerName, externalId): internalId
      !not-valid-provider
    provider.get(externalId): recordingData
    [GENIE]
    ex:provider.search(GenieCredentialsDto, externalId): search
      !not-found !timed-out
      !invalid
    ex:provider.download(UrlDto): BinaryData
      !not-found !timed-out
    db:metadata.set(internalId, metadata): void
      !timed-out !network-error
    os:storage.save(internalId, data): void
      !timed-out !network-error


[REQ] recording.get(GetRecordingDto): RecordingDto
    id.create(provider, externalId): internalRecordingId
      !not-valid-provider
    os:storage.load(internalId): recordingData
      !not-found !timed-out
      !network-error
    db:metadata.get(internalId): metadata
      !not-found !timed-out
      !network-error


[REQ] recording.addMetadata(GenieCredentialsDto): AddMetadataResponseDto
    recording.build(provider, externalRecordingId): internalRecordingId
      !not-valid-provider
    db:metadata.get(internalRecordingId): metadata
      !not-found !timed-out
      !network-error
    db:metadata.set(internalRecordingId, metadata): void
      !timed-out !network-error

[TYP] id: string
    a unique identifier for the recording generated by
    the system
[TYP] search: UrlDto[]
    a list of URLs returned by the provider's search
    endpoint that match the external recording ID
[TYP] provider: string
    the name of the provider from which the recording is
    sourced
[TYP] externalId: string
    the identifier used by the provider to reference the
    recording
[TYP] internalId: string
    a unique identifier for the recording generated by
    the system
[TYP] data: Uint8Array
    the binary data of the recording
[TYP] metadata: Record<string, Primitive>
    a key-value pair object containing metadata about
    the recording


[DTO] AddMetadataResponseDto {}

